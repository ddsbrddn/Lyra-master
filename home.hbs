{{!< default}}

{{!-- The tag above means: insert everything in this file
into the {body} of the default.hbs template --}}


{{#page}}
<header class="site-home-header">

    {{> header-background background=@site.cover_image}}
    {{!--Special header-image.hbs partial to generate the background image--}}

            <div class="inner">
                {{> "site-nav"}}

  		<div class="inner  gh-home-head-inner">

                <div class="gh-home-head-grid">
                    <div class="gh-members-head-content ">

                        <div class="gh-beta-badge"><span>beta</span></div>
                        <h1 class="gh-mainhead">Turn your audience into a business</h1>
                        <p class="gh-subhead">Use memberships and subscriptions to develop a direct relationship with
                            your
                            audience and generate predictable, recurring revenue to support your work.</p>
                        <div class="gh-btn-group"><a class="gh-btn gh-btn-green" href="/pricing/"><span>Get started
                                    now</span></a><a class="gh-btn gh-btn-outline" href="/pricing/"><span>See
                                    pricing</span></a></div>
                    </div>


		<div class="gh-home-head-graphic"><img loading="eager" src="/static/ghost-admin-home-header-0c20f780aac8e98af7a56076ae8228b8.png" alt="Ghost">
                    
 {{#if feature_image}}
                                        <figure class="kg-card kg-image-card kg-width-full">
                                            {{!-- This is a responsive image, it loads different sizes depending on device
                                        https://medium.freecodecamp.org/a-guide-to-responsive-images-with-ready-to-use-templates-c400bd65c433 --}}
                                            <img srcset="{{img_url feature_image size="s"}} 300w,
                                                    {{img_url feature_image size="m"}} 600w,
                                                    {{img_url feature_image size="l"}} 1000w,
                                                    {{img_url feature_image size="xl"}} 2000w" sizes="(max-width: 800px) 400px,
                                                (max-width: 1170px) 1170px,
                                                    2000px" src="{{img_url feature_image size="xl"}}" alt="{{title}}" class="kg-image" />
                                        </figure>
                                        {{else}}

                                                    <img loading="eager"
                                                    src="content/images/2020/03/ghost-admin-home-header-0c20f780aac8e98af7a56076ae8228b8.png"
                                                    alt="Ghost">
                                        {{/if}}
			</div>


                </div>
             </div>
        </div>

    </div>

</header>





<main id="site-main" class="site-main outer">
    <section class="inner gh-section post-full {{post_class}} {{#unless feature_image}}no-image{{/unless}}">

        {{!-- <section class="site-header-content post-full-content">
			<div class="post-content"> --}}

        {{!-- {{#if feature_image}}
				<figure class="kg-card kg-image-card kg-width-full"> --}}
        {{!-- This is a responsive image, it loads different sizes depending on device
                https://medium.freecodecamp.org/a-guide-to-responsive-images-with-ready-to-use-templates-c400bd65c433 --}}
